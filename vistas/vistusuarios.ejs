<script>
  function getValues() {
    var price = 0;
    var tax = 0;
    var total = 0;
    var priceVal = $('#id_price').val();
    var taxVal = $('#id_tax').val();
    var totalVal = $('#id_total').val();

    if (priceVal) {
      price = priceVal;
    }

    if (taxVal) {
      tax = taxVal;
    }

    if (totalVal) {
      total = totalVal;
    }

    $('#price').text('$' + price);
    $('#tax').text('$' + tax);
    $('#total').text('$' + total);
  }

  function getPrice(idProduct) {
    $.ajax({
      url: '/api/detailProduct/' + idProduct,
      type: 'GET',
      dataType: 'json',
      success: function (data) {
        var price = data[0]['price'];
        var quantity = $('#id_quantity').val();
        var discount = $('.js-userDiscount').data('discount');

        if (!discount) {
          discount = 0;
        }

        if (price == null) {
          price = 0;
        } else {
          console.log('price: ' + price, 'count: ' + quantity, 'discount: ' + discount);
          price = price * quantity * (1 - discount / 100);
          tax = price * 0.07;

          $('#price').text('$' + price);
          $('#id_price').val(price);

          $('#tax').text('$' + tax.toFixed(2));
          $('#id_tax').val(tax.toFixed(2));

          $('#total').text('$' + (price + tax).toFixed(2));
          $('#id_total').val((price + tax).toFixed(2));
        }

      }
    });
  }

  function getPreview(idProduct) {
    var width = $('#id_slabGrossWidthOptions option:selected').text().replace('"', '');
    var height = $('#id_height option:selected').text().replace('"', '');

    $.ajax({
      url: '/products/detailProduct/' + idProduct + "/?width=" + width + "&height=" + height,
      type: 'GET',
      success: function (data) {
        $('.js-preview').html(data);
      }
    });
  }

  $('#id_slabGrossWidthOptions, #id_height').change(function () {
    var idProduct = $('#id_product').val();

    getPreview(idProduct);
  });

  $('#id_product').change(function () {
    idProduct = $(this).val();

    getPreview(idProduct);
    getPrice(idProduct);
  });

  $('#id_quantity').change(function () {
    idProduct = $('#  ').val();

    getPrice(idProduct);
  });

  $('.js-textures').click(function () {
    var id = $(this).data('value');
    var label = $(this).data('label');

    console.log(label);
    $('#id_woodVeneerOptions').val(id);
    $('.js-textureLabel').text(label);
  });

  // load
  getValues();
</script>
</body>

