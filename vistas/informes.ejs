                    { extend: 'copy', className: 'btn btn-light' },
                    { extend: 'csv', className: 'btn btn-light' },
                    { extend: 'excel', className: 'btn btn-light' }
                ]
            });

            // Refilter the table
            $('#min, #max').on('change', function () {
                table.draw();
            });

            $('.DataTables_Table_0_filter').find('input').addClass('form-control');
        });
